                android:textSize="20dp" />

            <TextView
                android:id="@+id/txtID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
